{
  "name": "@januswel/base32",
  "version": "0.0.3",
  "description": "Base32 encoder/decoder for hex string",
  "main": "lib/index.js",
  "scripts": {
    "prepack": "npm run prepack:babel && npm run prepack:flow",
    "prepack:babel": "rm -rf lib && NODE_ENV=production babel src -d lib",
    "prepack:flow": "flow-copy-source src lib",
    "test": "mocha --require @babel/register test",
    "type-check": "flow"
  },
  "author": "januswel <janus.wel.3@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/januswel/base32"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-plugin-unassert": "2.1.2",
    "babel-preset-power-assert": "3.0.0",
    "flow-bin": "0.62.0",
    "flow-copy-source": "1.2.1",
    "flow-typed": "2.5.1",
    "mocha": "5.2.0",
    "power-assert": "1.6.1",
    "prettier": "1.15.3"
  }
}
