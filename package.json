{
  "name": "@januswel/base32",
  "description": "Base32 encoder/decoder for hex string",
  "author": "januswel <janus.wel.3@gmail.com>",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/januswel/base32"
  },
  "main": "dist/index.js",
  "bin": {
    "base32": "bin/base32"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src -d dist && flow-copy-source src dist",
    "prebuild": "rm -rf dist",
